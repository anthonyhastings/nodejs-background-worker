{
  "extends": ["airbnb-base", "prettier"],
  "root": true,
  "env": {
    "es2022": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "import/extensions": [
      "error",
      {
        "js": "always",
        "mjs": "always"
      }
    ]
  }
}
